	<uses-sdk />
	<application android:label="EventsIP" android:icon="@drawable/Icon" android:theme="@android:style/Theme.NoTitleBar">
		<meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id" />
